{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
     
    },
    "ircolor-uikit#build": {
      "inputs": []
    },
    "ircolor-uikit#dev": {
      "dependsOn": ["ircolor-icons#build","build"],
      "inputs": []
    },
    "docs#dev": {
      "dependsOn": ["ircolor-uikit#build"]
    }
  }
}
